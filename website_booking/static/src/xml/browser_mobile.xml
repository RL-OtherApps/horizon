<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="website_booking.browser_mobile">
        <div class="row mt16 mb16">
            <div class="col s6" align="center">
                <a id="todat" class="waves-effect waves-light btn"><t t-esc="widget.today.format('DD/MM');"/></a>
            </div>
            <div class="col s6" align="center">
                <a id="tomorrow" class="waves-effect waves-light btn"><t t-esc="widget.tomorrow.format('DD/MM');"/></a>
            </div>
        </div>
    </t>
</templates>